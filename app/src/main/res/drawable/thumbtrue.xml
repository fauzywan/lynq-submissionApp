<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid android:color="@color/secondary_color" />
            <size
                android:width="6dp"
                android:height="6dp" />
            <stroke
                android:width="2dp"
                android:color="@color/primary_color" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_moon"
        android:top="6dp"
        android:left="6dp"
        android:right="6dp"
        android:bottom="6dp"/>
</layer-list>